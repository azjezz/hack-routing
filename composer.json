{
  "name": "facebook/hack-router",
  "description": "URI routing for Hack",
  "license": "MIT",
  "keywords": ["hack", "router", "routing", "hhvm"],
  "homepage": "https://github.com/hhvm/hack-router",
  "require": {
    "php": "^8.0",
    "azjezz/psl": "^1.7.2",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "cache/apcu-adapter": "^1.1",
    "cache/array-adapter": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "HackRouting\\": "src/"
    },
    "files": [
      "src/PatternParser/tokenize.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
